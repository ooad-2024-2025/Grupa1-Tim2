            "destination": "wwwroot/lib/bootstrap/",
            "files": [
                "css/bootstrap.min.css", // PROMIJENJENO: Uklonjen 'dist/'
                "css/bootstrap.min.css.map", // PROMIJENJENO: Uklonjen 'dist/'
                "js/bootstrap.bundle.min.js", // PROMIJENJENO: Uklonjen 'dist/'
